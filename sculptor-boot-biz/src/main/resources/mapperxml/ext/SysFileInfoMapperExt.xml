<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cdk8s.sculptor.mapper.ext.SysFileInfoMapperExt">

	<!--=====================================查询业务 start=====================================-->

	<select id="selectDistinctFileSuffix" resultMap="com.cdk8s.sculptor.mapper.SysFileInfoMapper.BaseResultMap">
		select DISTINCT(file_suffix)
		from sys_file_info
	</select>

	<!--=====================================查询业务 end=====================================-->
	<!--=====================================操作业务 start=====================================-->


	<!--=====================================操作业务 end=====================================-->

</mapper>
